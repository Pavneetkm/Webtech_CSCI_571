<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <ImageButton
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/star"
                android:id="@+id/imageButton"
                android:layout_gravity="end"
                android:layout_marginLeft="150dp"
                />


            </TableRow>


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="Bill ID:"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:id="@+id/textView3"
                android:textStyle="bold"
                android:layout_weight="20"/>

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:textAllCaps="true"
                android:layout_weight="80"
                android:id="@+id/billid" />



        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="Title:"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:id="@+id/textView4"
                android:textStyle="bold"
                android:layout_weight="20"/>

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="80"
                android:id="@+id/title" />


        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="Bill Type:"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:id="@+id/textView5"
                android:textStyle="bold"
                android:layout_weight="20"/>

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:textAllCaps="true"
                android:layout_weight="80"
                android:id="@+id/billtype" />


        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="Sponsor:"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:id="@+id/textView6"
                android:textStyle="bold"
                android:layout_weight="20"/>

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_weight="80"
                android:id="@+id/sponsor" />



        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="Chamber:"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:id="@+id/textView7"
                android:textStyle="bold"
                android:layout_weight="20"/>

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_weight="80"
                android:id="@+id/chamber" />



        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="Introduced On:"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:id="@+id/textView9"
                android:textStyle="bold"
                android:layout_weight="20"/>

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_weight="80"
                android:id="@+id/introducedon" />



        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="Congress URL:"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:id="@+id/textView10"
                android:textStyle="bold"
                android:layout_weight="20"/>

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_weight="80"
                android:id="@+id/url" />



        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="Version Status:"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:id="@+id/textView11"
                android:textStyle="bold"
                android:layout_weight="20"/>

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_weight="80"
                android:id="@+id/status" />



        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="Bill:"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:id="@+id/textView12"
                android:textStyle="bold"
                android:layout_weight="20"/>

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_weight="80"
                android:id="@+id/bill" />



        </TableRow>



    </TableLayout>


</LinearLayout>